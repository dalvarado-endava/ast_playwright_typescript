{
  "name": "ast_playwright_typescript",
  "version": "1.0.0",
  "description": "Automation software testing in typescript using Playwright",
  "main": "index.js",
  "scripts": {
    "test:e2e:chrome": "playwright test --config=e2e.config.ts --project=Chrome",
    "test:e2e:firefox": "playwright test --config=e2e.config.ts --project=Firefox",
    "test:e2e:webkit": "playwright test --config=e2e.config.ts --project=Webkit",
    "test:e2e:chrome:allure": "playwright test --config=e2e.config.ts --project=Chrome --reporter=allure-playwright,line",
    "test:show-report": "playwright show-report",
    "allure-report": "npx allure generate ./allure-results && allure open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalvarado-endava/ast_playwright_typescript.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dalvarado-endava/ast_playwright_typescript/issues"
  },
  "homepage": "https://github.com/dalvarado-endava/ast_playwright_typescript#readme",
  "dependencies": {
    "@playwright/test": "^1.22.2",
    "allure-playwright": "^2.0.0-beta.18"
  }
}
